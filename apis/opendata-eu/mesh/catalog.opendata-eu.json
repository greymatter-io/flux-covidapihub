{
  "clusterName": "apis.opendata-eu",
  "zoneName": "default.zone",
  "name": "EU Open Data Covid Dataset",
  "version": "1.0",
  "owner": "European Centre for Disease Prevention and Control",
  "capability": "health",
  "runtime": "GO",
  "documentation": "https://www.ecdc.europa.eu/en/publications-data/download-todays-data-geographic-distribution-covid-19-cases-worldwide",
  "prometheusJob": "apis.opendata-eu",
  "minInstances": 1,
  "maxInstances": 2,
  "authorized": true,
  "enableInstanceMetrics": true,
  "enableHistoricalMetrics": true,
  "metricsPort": 8080
}
