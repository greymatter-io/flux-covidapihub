{
  "zone_key": "default.zone",
  "listener_key": "edge.ingress.listener",
  "domain_keys": [
    "edge.ingress.domain"
  ],
  "name": "ingress",
  "ip": "0.0.0.0",
  "port": 8443,
  "protocol": "http_auto",
  "tracing_config": null,
  "active_http_filters": [
    "gm.observables",
    "gm.metrics",
    "gm.oidc-validation",
    "envoy.rbac"
  ],
  "http_filters": {
    "gm_metrics": {
      "metrics_port": 8080,
      "metrics_host": "0.0.0.0",
      "metrics_dashboard_uri_path": "/metrics",
      "metrics_prometheus_uri_path": "/prometheus",
      "metrics_ring_buffer_size": 4096,
      "prometheus_system_metrics_interval_seconds": 15,
      "metrics_key_function": "depth"
    },
    "gm_observables": {},
    "gm_oidc-validation": {
      "accessToken": {
        "location": "header",
        "key": "access_token"
      },
      "provider": "https://accounts.google.com",
      "enforce": false
    },
    "envoy_rbac": {
      "rules": {
        "action": 0,
        "policies": {
          "0": {
            "permissions": [
              {
                "any": true
              }
            ],
            "principals": [
              {
                "header": {
                  "name": "access_token",
                  "present_match": true
                }
              }
            ]
          },
          "1": {
            "permissions": [
              {
                "header": {
                  "name": ":method",
                  "exact_match": "GET"
                }
              }
            ],
            "principals": [
              {
                "any": true
              }
            ]
          }
        }
      }
    }
  }
}