{
  "zone_key": "default.zone",
  "domain_key": "edge.ingress.domain",
  "name": "*",
  "port": 8443,
  "redirects": null,
  "gzip_enabled": false,
  "ssl_config": {
    "cert_key_pairs": [
      {
        "certificate_path": "/etc/tls/edge.svc.crt",
        "key_path": "/etc/tls/edge.svc.key"
      }
    ],
    "trust_file": "/etc/tls/ca.crt",
    "require_client_certs": false
  },
  "cors_config": null,
  "aliases": null,
  "force_https": true
}
