{
  "zone_key": "default.zone",
  "listener_key": "edge.login.listener",
  "domain_keys": ["edge.login.domain"],
  "name": "ingress",
  "ip": "0.0.0.0",
  "port": 9443,
  "protocol": "http_auto",
  "tracing_config": null,
  "active_http_filters": [
    "gm.metrics",
    "gm.oidc-validation",
    "gm.oidc-authentication"
  ],
  "http_filters": {
    "gm_oidc-validation": {
      "accessToken": {
        "location": "cookie",
        "key": "access_token"
      },
      "provider": "https://accounts.google.com",
      "enforce": false
    },
    "gm_oidc-authentication": {
      "accessToken": {
        "location": "cookie",
        "key": "access_token",
        "cookieOptions": { "httpOnly": true, "maxAge": "1h", "domain":"*.covidapihub.io", "path": "/" }
      },
      "idToken": {
        "location": "cookie",
        "key": "id_token",
        "cookieOptions": { "httpOnly": true, "maxAge": "1h", "domain":"*.covidapihub.io", "path": "/" }
      },
      "serviceUrl": "https://login.covidapihub.io",
      "callbackPath": "/oauth",
      "provider": "https://accounts.google.com",
      "clientId": "REDACTED",
      "clientSecret": "REDACTED",
      "additionalScopes": ["profile", "email"]
    }
  }
}
