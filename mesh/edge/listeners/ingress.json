{
  "zone_key": "default.zone",
  "listener_key": "edge.ingress.listener",
  "domain_keys": [
    "edge.ingress.domain"
  ],
  "name": "ingress",
  "ip": "0.0.0.0",
  "port": 8443,
  "protocol": "http_auto",
  "tracing_config": null,
  "active_http_filters": [
    "gm.observables",
    "gm.metrics",
    "gm.oidc-validation",
    "gm.oidc-authentication"
  ],
  "http_filters": {
    "gm_metrics": {
      "metrics_port": 8080,
      "metrics_host": "0.0.0.0",
      "metrics_dashboard_uri_path": "/metrics",
      "metrics_prometheus_uri_path": "/prometheus",
      "metrics_ring_buffer_size": 4096,
      "prometheus_system_metrics_interval_seconds": 15,
      "metrics_key_function": "depth"
    },
    "gm_observables": {
      "topic": "edge",
      "eventTopic": "observables",
      "useKafka": true,
      "kafkaServerConnection": "b-3.covidapihub-observable.y8lgoi.c6.kafka.us-east-1.amazonaws.com:9094,b-1.covidapihub-observable.y8lgoi.c6.kafka.us-east-1.amazonaws.com:9094,b-2.covidapihub-observable.y8lgoi.c6.kafka.us-east-1.amazonaws.com:9094",
      "useKafkaTLS": true

    },
    "gm_oidc-validation": {
      "accessToken": {
        "location": "cookie",
        "key": "access_token"
      },
      "provider": "https://accounts.google.com",
      "enforce": false
    },
    "gm_oidc-authentication": {
      "accessToken": {
        "location": "cookie",
        "key": "access_token",
        "cookieOptions": { "httpOnly": true, "maxAge": "1h", "domain":"*.covidapihub.io", "path": "/" }
      },
      "idToken": {
        "location": "cookie",
        "key": "id_token",
        "cookieOptions": { "httpOnly": true, "maxAge": "1h", "domain":"*.covidapihub.io", "path": "/" }
      },
      "serviceUrl": "https://covidapihub.io",
      "callbackPath": "/oauth",
      "provider": "https://accounts.google.com",
      "clientId": "REDACTED",
      "clientSecret": "REDACTED",
      "additionalScopes": ["profile", "email"]
    },
    "envoy_rbac": {
      "rules": {
        "action": 0,
        "policies": {
          "0": {
            "permissions": [
              {
                "any": true
              }
            ],
            "principals": [
              {
                "header": {
                  "name": "access_token",
                  "present_match": true
                }
              }
            ]
          },
          "1": {
            "permissions": [
              {
                "header": {
                  "name": ":method",
                  "exact_match": "GET"
                }
              }
            ],
            "principals": [
              {
                "any": true
              }
            ]
          }
        }
      }
    }
  }
}