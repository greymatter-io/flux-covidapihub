{
    "zone_key": "default.zone",
    "listener_key": "dashboard.ingress.listener",
    "domain_keys": [
        "dashboard.ingress.domain"
    ],
    "name": "ingress",
    "ip": "0.0.0.0",
    "port": 8443,
    "protocol": "http_auto",
    "secret": {
        "secret_key": "dashboard.identity",
        "secret_name": "spiffe://covidapihub.io/ns/sense/sa/dashboard",
        "secret_validation_name": "spiffe://covidapihub.io",
        "subject_names": [
            "spiffe://covidapihub.io/ns/ingress/sa/private"
        ],
        "ecdh_curves": [
            "X25519:P-256:P-521:P-384"
        ]
    },
    "active_http_filters": [
        "gm.metrics",
        "gm.observables"
    ],
    "http_filters": {
        "gm_metrics": {
            "metrics_port": 8080,
            "metrics_host": "0.0.0.0",
            "metrics_dashboard_uri_path": "/metrics",
            "metrics_prometheus_uri_path": "/prometheus",
            "metrics_ring_buffer_size": 4096,
            "prometheus_system_metrics_interval_seconds": 15,
            "metrics_key_function": "depth"
        },
        "gm_observables": {}
    },
    "tracing_config": null
}
