{
    "zone_key": "default.zone",
    "listener_key": "website.listener",
    "domain_keys": [
        "website.wildcard.domain"
    ],
    "name": "website",
    "ip": "0.0.0.0",
    "port": 8443,
    "protocol": "http_auto",
    "secret": {
        "secret_key": "website.identity",
        "secret_name": "spiffe://covidapihub.io/ns/website/sa/website",
        "secret_validation_name": "spiffe://covidapihub.io",
        "subject_names": [
            "spiffe://covidapihub.io/ns/ingress/sa/public"
        ],
        "ecdh_curves": [
            "X25519:P-256:P-521:P-384"
        ]
    },
    "active_http_filters": [
        "gm.metrics",
        "gm.observables"
    ],
    "http_filters": {
        "gm_metrics": {
            "metrics_port": 8080,
            "metrics_host": "0.0.0.0",
            "metrics_dashboard_uri_path": "/metrics",
            "metrics_prometheus_uri_path": "/prometheus",
            "metrics_ring_buffer_size": 4096,
            "prometheus_system_metrics_interval_seconds": 15,
            "metrics_key_function": "depth"
        },
        "gm_observables": {
            "emitFullResponse": false,
            "useKafka": false,
            "useKafkaTLS": true,
            "eventTopic": "observables",
            "enforceAudit": false,
            "topic": "website",
            "kafkaZKDiscover": false,
            "kafkaServerConnection": "b-3.production-observables.5b3bmo.c4.kafka.us-east-1.amazonaws.com:9094,b-2.production-observables.5b3bmo.c4.kafka.us-east-1.amazonaws.com:9094,b-1.production-observables.5b3bmo.c4.kafka.us-east-1.amazonaws.com:9094"
        }
    },
    "tracing_config": null
}
