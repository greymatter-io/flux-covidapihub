{
    "zone_key": "default.zone",
    "proxy_key": "website.proxy",
    "domain_keys": [
        "website.wildcard.domain"
    ],
    "listener_keys": [
        "website.listener"
    ],
    "name": "website.website",
    "listeners": null,
    "active_proxy_filters": [
      "envoy.rbac"
    ],
    "proxy_filters": {
	"envoy_rbac": {
            "rules": {
		"action": 0,
		"policies": {
		    "0": {
			"permissions": [
			    {
				"header": {
				    "name": ":method",
				    "exact_match": "GET"
				}
			    }
			],
			"principals": [
			    {
				"any": true
			    }
			]
		    }
		}
	    }
	}
    }
}
